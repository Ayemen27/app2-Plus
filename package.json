    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --logLevel=error",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --external:./vite --external:../vite.config --bundle --format=esm --outfile=dist/index.js --log-level=error && esbuild server/vite.ts --platform=node --format=esm --outfile=dist/vite.js --log-level=error && cp vite.config.ts dist/vite.config.ts",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "clean": "rm -rf dist node_modules/.vite",
    "db:push": "drizzle-kit push",
    "migrate": "drizzle-kit push",
    "android:deploy": "bash scripts/build-and-deploy.sh",
    "android:sync-version": "node scripts/sync-version.js",
    "android:build": "npm run android:sync-version && cd android && chmod +x gradlew && ./gradlew assembleDebug",
    "android:clean": "cd android && ./gradlew clean"
  },
